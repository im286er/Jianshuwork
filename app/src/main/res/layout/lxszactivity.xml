<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/listView7"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="47dp" />

    <TextView
        android:text="离线设置"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textureView4"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <Switch
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/switch9"
        android:layout_alignTop="@+id/listView7"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />
    <Switch
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/switch10"
        android:layout_below="@+id/switch9"
        android:layout_alignLeft="@+id/switch9"
        android:layout_alignStart="@+id/switch9" />
    <Switch
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/switch11"
        android:layout_below="@+id/switch10"
        android:layout_alignLeft="@+id/switch10"
        android:layout_alignStart="@+id/switch10" />



</RelativeLayout>
